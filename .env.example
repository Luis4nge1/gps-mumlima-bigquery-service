# Configuración del servidor
PORT=3000
HOST=localhost
NODE_ENV=development

# Configuración de Redis Cloud
REDIS_HOST=your-redis-cloud-host.redislabs.com
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-cloud-password
REDIS_DB=0
REDIS_KEY_PREFIX=gps:

# Configuración GPS
GPS_LIST_KEY=gps:history:global
GPS_BATCH_SIZE=1000
GPS_OUTPUT_FILE=tmp/gps_data.txt
GPS_BACKUP_ENABLED=true
GPS_BACKUP_PATH=tmp/backup/
GPS_BACKUP_MAX_FILES=3
GPS_BACKUP_ONLY_ON_SUCCESS=true

# Configuración del Scheduler
SCHEDULER_INTERVAL_MINUTES=5
SCHEDULER_ENABLED=true
SCHEDULER_TIMEZONE=America/Lima
SCHEDULER_MAX_CONCURRENT=1

# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=./service-account.json
GCP_PROJECT_ID=your-gcp-project-id
GCS_BUCKET_NAME=your-gcs-bucket-name
GCS_GPS_PREFIX=gps-data/
GCS_MOBILE_PREFIX=mobile-data/
GCS_REGION=us-central1

# BigQuery Configuration
BIGQUERY_DATASET_ID=location_data
BIGQUERY_GPS_TABLE_ID=gps_records
BIGQUERY_MOBILE_TABLE_ID=mobile_records
BIGQUERY_LOCATION=US
BIGQUERY_BATCH_SIZE=1000

# Redis Data Sources
REDIS_GPS_KEY_PATTERN=gps:last:*
REDIS_MOBILE_KEY_PATTERN=mobile:last:*

# Recovery Configuration
GCS_RECOVERY_ENABLED=true
GCS_MAX_RETRY_ATTEMPTS=3
GCS_CLEANUP_PROCESSED_FILES=true

# Development Mode (simulated)
GCP_SIMULATION_MODE=false
GCS_LOCAL_SIMULATION_PATH=tmp/gcs-simulation/
BIGQUERY_LOCAL_SIMULATION_PATH=tmp/bigquery-simulation/

# Configuración de Logging
LOG_LEVEL=info
LOG_FORMAT=combined
LOG_FILE=
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# Configuración de Monitoreo
MONITORING_ENABLED=false
METRICS_PORT=9090
HEALTH_CHECK_PATH=/health

# Configuración de Seguridad
API_KEY=
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Configuración de Backup Local (Procesamiento Atómico)
BACKUP_MAX_RETRIES=3
BACKUP_RETENTION_HOURS=24
BACKUP_STORAGE_PATH=tmp/atomic-backups/
BACKUP_CLEANUP_INTERVAL_MINUTES=60
ATOMIC_PROCESSING_ENABLED=true
ATOMIC_PROCESSING_TIMEOUT_MS=30000

# Configuración de Migración Gradual
MIGRATION_ENABLED=true
NEW_FLOW_ENABLED=false
HYBRID_MODE=false
MIGRATION_PHASE=legacy
ROLLBACK_ENABLED=true
ROLLBACK_THRESHOLD=3
ROLLBACK_CONSECUTIVE_FAILURES=3
ROLLBACK_ERROR_RATE=0.5
ROLLBACK_PERFORMANCE_THRESHOLD=2.0
ROLLBACK_COOLDOWN_MINUTES=30
PERFORMANCE_LOGGING_ENABLED=true
DETAILED_PERFORMANCE_METRICS=false
COMPARISON_LOGGING=true
COMPARISON_ENABLED=false
COMPARISON_SAMPLE_RATE=1.0
COMPARISON_TOLERANCE=0.05
METRICS_RETENTION_DAYS=7